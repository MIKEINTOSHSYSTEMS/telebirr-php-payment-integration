# Telebirr API Configuration
# Test Environment
BASE_URL=https://developerportal.ethiotelebirr.et:38443/apiaccess/payment/gateway
WEB_BASE_URL=https://developerportal.ethiotelebirr.et:38443/payment/web/paygate?

# Production Environment (uncomment for production)
# BASE_URL=https://telebirrappcube.ethiomobilemoney.et:38443/apiaccess/payment/gateway
# WEB_BASE_URL=https://telebirrappcube.ethiomobilemoney.et:38443/payment/web/paygate?

# App Credentials
FABRIC_APP_ID=your-fabric-app-id-here
APP_SECRET=your-app-secret-here
MERCHANT_APP_ID=your-merchant-app-id-here
MERCHANT_CODE=your-merchant-code-here

# Private Key (store in config/keys/private_key.pem)
# Public Key (store in config/keys/public_key.pem)

# Application Settings
APP_NAME="Telebirr Payment Demo"
APP_URL=http://your-app-url-here
DEBUG_MODE=true
TIMEZONE=Africa/Addis_Ababa

# Database Configuration (optional, for storing transactions)
DB_HOST=localhost
DB_NAME=your-database-name
DB_USER=your-database-user
DB_PASS=your-database-password
DB_CHARSET=utf8mb4

# Notification Settings
NOTIFY_URL=http://your-notify-url-here
REDIRECT_URL=http://your-success-url-here
FAILURE_URL=http://your-failure-url-here

# Logging
LOG_PATH=logs/payment.log
LOG_LEVEL=DEBUG
